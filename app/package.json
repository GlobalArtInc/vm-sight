    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "28.1.8",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.202",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.7",
