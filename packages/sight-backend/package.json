{
  "name": "@vm-sight/sight-backend",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "VM-Sight backend",
  "main": "server.js",
  "author": "GlobalArt, Inc",
  "license": "ISC",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "dev": "cross-env NODE_ENV=development nodemon",
    "start": "ts-node ./src/server.ts --env=prod",
    "db:migrate:test": "cd src && npx sequelize-cli db:migrate --env development --config ./configs/config.json",
    "db:migrate:test:undo": "cd src && npx sequelize-cli db:migrate:undo:all --env development --config ./configs/config.json",
    "db:migrate:test:redo": "yarn db:migrate:test:undo && yarn db:migrate:test",
    "build": "tsc && tsc-alias",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "fix": "yarn run lint -- --fix"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "docker-hub-api": "^0.8.0",
    "dockerode": "^3.2.1",
    "dotenv": "^16.0.1",
    "dotenv-defaults": "^5.0.0",
    "envalid": "^7.3.1",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.3",
    "express-ws": "^4.0.0",
    "helmet": "^5.1.0",
    "hpp": "^0.2.3",
    "http-proxy-middleware": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.0.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "optimist": "^0.6.1",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-oauth2-client-password": "^0.1.2",
    "path": "^0.12.7",
    "sequelize": "^6.19.2",
    "sequelize-isunique-validator": "^1.1.1",
    "socker.io": "^0.0.0-alpha-beta",
    "socket.io": "^4.0.0",
    "sqlite3": "^5.0.8",
    "stream": "^0.0.2",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^4.1.6",
    "tsc-alias": "^1.6.7",
    "tslog": "^3.2.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/dockerode": "^3.3.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/hpp": "^0.2.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.6",
    "@types/sequelize": "^4.28.13",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cross-env": "^7.0.3",
    "del": "^5.1.0",
    "dotenv": "^16.0.1",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.0.1",
    "https-proxy-agent": "^5.0.0",
    "mock-aws-s3": "^4.0.1",
    "nock": "^13.0.10",
    "node-config": "^0.0.2",
    "node-gyp": "^9.0.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.6.2",
    "sequelize-isunique-validator": "^1.1.1",
    "tsc-alias": "^1.6.7",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.4.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-stream": "^5.2.1"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ]
    }
  }
}
