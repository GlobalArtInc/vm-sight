{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es6",
    "module": "CommonJS",
    "outDir": "./backend",
    "baseUrl": "./",
    "moduleResolution": "Node",
    "alwaysStrict": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": [
    "*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}